<h2>Login</h2>
<h3><%= form_for :user, url: {action: :verify} do |form| %>
	<p><label_for="username">UserName:</label>
	<%= form.text_field :username,  size: 20 %></p>
	<p><label_for = "password">Password:</label>
	<%= form.password_field :password,  size: 20 %></p
	<p><%= submit_tag "Login" %></p></h3>
<% end %>

